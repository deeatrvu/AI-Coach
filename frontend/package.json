{
  "name": "medrep-coach-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "esbuild src/main.ts --bundle --sourcemap --outfile=public/bundle.js",
    "watch": "esbuild src/main.ts --bundle --sourcemap --outfile=public/bundle.js --watch",
    "typecheck": "tsc --noEmit",
    "dev": "concurrently \"npm run watch\" \"serve public\""
  },
  "dependencies": {
    "jquery": "^3.7.1"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.29",
    "concurrently": "^9.2.1",
    "esbuild": "^0.21.5",
    "serve": "^14.2.5",
    "typescript": "^5.4.5"
  }
}
